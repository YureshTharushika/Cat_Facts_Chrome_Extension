{
    "manifest_version": 3,
    "name": "Cat Facts",
    "version": "1.0.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "assets/icons/icon16.png",
        "32": "assets/icons/icon32.png",
        "128": "assets/icons/icon128.png"
      }
    },
    "permissions": [
      "tabs",
      "scripting",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  